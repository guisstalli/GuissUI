{
  "id": "guiss-dev",
  "realm": "guiss-dev",
  "displayName": "GUISS",
  "displayNameHtml": "<strong>GUISS</strong> Development",
  "enabled": true,

  "sslRequired": "none",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,

  "verifyEmail": true,
  "rememberMe": true,

  "bruteForceProtected": true,
  "permanentLockout": false,
  "failureFactor": 10,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,

  "internationalizationEnabled": true,
  "supportedLocales": ["en", "fr"],
  "defaultLocale": "fr",

  "loginTheme": "keycloak",
  "accountTheme": "keycloak.v2",
  "adminTheme": "keycloak.v2",
  "emailTheme": "keycloak",

  "accessTokenLifespan": 300,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "offlineSessionIdleTimeout": 2592000,
  "offlineSessionMaxLifespanEnabled": false,

  "accessCodeLifespan": 60,
  "accessCodeLifespanUserAction": 300,
  "accessCodeLifespanLogin": 1800,

  "defaultSignatureAlgorithm": "RS256",

  "smtpServer": {
    "host": "mailhog",
    "port": "1025",
    "from": "noreply@guissdev.com",
    "fromDisplayName": "GUISS",
    "ssl": "false",
    "starttls": "false",
    "auth": "false"
  },

  "passwordPolicy": "length(8) and digits(1) and upperCase(1) and specialChars(1) and notUsername",

  "eventsEnabled": true,
  "eventsListeners": ["jboss-logging"],
  "adminEventsEnabled": true,
  "adminEventsDetailsEnabled": true,
  "enabledEventTypes": [
    "LOGIN",
    "LOGIN_ERROR",
    "LOGOUT",
    "LOGOUT_ERROR",
    "REFRESH_TOKEN",
    "REFRESH_TOKEN_ERROR",
    "UPDATE_PASSWORD",
    "UPDATE_PASSWORD_ERROR"
  ],

  "roles": {
    "realm": [
      { "name": "ADMIN", "description": "Administrateur de la plateforme" },
      { "name": "DOCTOR", "description": "MÃ©decin" },
      { "name": "TECHNICIAN", "description": "Technicien" },
      { "name": "DATA_ENTRY", "description": "Agent de saisie" },
      { "name": "PARTNER_USER", "description": "Utilisateur partenaire" }
    ]
  },

  "groups": [
    {
      "name": "Administrators",
      "path": "/Administrators",
      "realmRoles": ["ADMIN"]
    },
    {
      "name": "MedicalStaff",
      "path": "/MedicalStaff",
      "subGroups": [
        {
          "name": "Doctors",
          "path": "/MedicalStaff/Doctors",
          "realmRoles": ["DOCTOR"]
        },
        {
          "name": "Technicians",
          "path": "/MedicalStaff/Technicians",
          "realmRoles": ["TECHNICIAN"]
        }
      ]
    },
    {
      "name": "DataEntry",
      "path": "/DataEntry",
      "realmRoles": ["DATA_ENTRY"]
    },
    {
      "name": "Partners",
      "path": "/Partners",
      "realmRoles": ["PARTNER_USER"]
    }
  ],

  "defaultDefaultClientScopes": [
    "profile",
    "email",
    "roles",
    "web-origins",
    "openid"
  ],

  "defaultOptionalClientScopes": [
    "phone",
    "offline_access"
  ],

  "clients": [
    {
      "clientId": "guiss-frontend",
      "name": "GUISS Frontend",
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": false,
      "implicitFlowEnabled": false,

      "rootUrl": "http://localhost:3000",
      "redirectUris": [
        "http://localhost:3000/*",
        "http://localhost:4200/*",
        "http://localhost:5173/*",
        "http://localhost:8000/*",
        "http://localhost:8001/*",
        "https://oauth.pstmn.io/v1/callback"
      ],
      "webOrigins": ["+"],

      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost:3000/*"
      }
    },

    {
      "clientId": "guiss-admin-client",
      "name": "GUISS Admin Service Account",
      "protocol": "openid-connect",
      "publicClient": false,
      "serviceAccountsEnabled": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,

      "secret": "dev-guiss-admin-secret-change-in-prod"
    },

    {
      "clientId": "guiss-resource-server",
      "name": "GUISS Resource Server",
      "protocol": "openid-connect",
      "bearerOnly": true,
      "publicClient": false
    }
  ],

